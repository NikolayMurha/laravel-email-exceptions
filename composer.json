{
  "name": "nikolaymurha/laravel-email-exceptions",
  "description": "Email Exceptions from your laravel application",
  "license": "MIT",
  "keywords": [
    "email",
    "exception",
    "laravel",
    "laravel-5-package",
    "exception-handling",
    "debugging"
  ],
  "authors": [
    {
      "name": "Aaron Brigham",
      "email": "abrigham1@gmail.com"
    },
    {
      "name": "Nikolay Murha",
      "email": "work@murga.kiev.ua"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.0 || ~6.0 ",
    "tijsverkoyen/css-to-inline-styles": "^2.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "2.*",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^0.9.7",
    "composer/composer": "^1.9"
  },
  "suggest": {
    "laravel/lumen-framework": "^5.0",
    "laravel/framework": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Abrigham\\LaravelEmailExceptions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Abrigham\\LaravelEmailExceptions\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Abrigham\\LaravelEmailExceptions\\EmailExceptionsServiceProvider"
      ]
    }
  }
}
